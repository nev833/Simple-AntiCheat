local hum = script.Parent:WaitForChild("Humanoid")

while true do
	if hum.WalkSpeed >= 20 then
		game.Players.LocalPlayer:Kick("You Have Been Banned For 4 weeks reason: The anticheat has found A Speed Boost Cheat.")
	end
	wait(10)
	if hum.JumpPower >= 60 then
		game.Players.LocalPlayer:Kick("You Have Been Banned For 4 weeks reason: The anticheat has found A Jump Boost Cheat.")
	end
end
